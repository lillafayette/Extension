{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1.0",
  "web_accessible_resources": ["toggleBold.js"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "toggleBold.js"]
    }
  ]
}

